pipeline {
    agent any
    tools {
        jdk "java-21"
        maven "maven"
    }
    stages {
        stage ('git checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/chethan-nexon/dock-jenkins.git'
            }
        } 
        stage ('compile'){
            steps {
                sh "mvn compile"
            }
        }
        stage ('build') {
            steps {
                sh "mvn clean install"
            }
        }

          
    }

}
